import { Directive, ElementRef, Input } from '@angular/core';

@Directive({
  selector: '{{tagName}}'
})
export class {{componentName}}Directive {
  @ViewChild('el') private el: ElementRef;

  {{#each attributes}}
  @Input() 
  set {{name}}(value: {{type}}) {
    this.el.nativeElement.{{name}} = value;
  }
  {{/each}}
}